ifc: a owl:Ontology ;
    cc:license "http://creativecommons.org/licenses/by/3.0/" ;
    dce:abstract "This Ontology is automatically created from the EXPRESS schema 'IFC4X3_ADD2' using the IFCExpress2OWL_Proces custom converter developed  by Carlos Ramonell (carlos.ramonell@upc.edu)" ;
    dce:creator "Carlos Ramonell Cazador (carlos.ramonell@upc.edu)" ;
    dce:date "2024/03/27" ;
    dce:description "OWL ontology to describe processes in the built environment. It is based on the Proces ontology descibed in IFC (Insutry Foundation Classes) schema." ;
    dce:identifier "Proces Ontology" ;
    dce:language "en" ;
    dce:title "Proces Ontology" ;
    vann:preferredNamespacePrefix "po" ;
    vann:preferredNamespaceUri "https://w3id.org/po-ifc#" ;
    owl:versionIRI ifc: ;
    owl:versionInfo "1.0" .

ifc:Procedure a owl:Class ;
    rdfs:subClassOf ifc:Process ;
    owl:disjointWith ifc:Event,
        ifc:Task .

ifc:SchedulingTime a owl:Class ;
    rdfs:subClassOf [ a owl:Class ;
            owl:unionOf ( ifc:EventTime ifc:TaskTime ) ] .

ifc:Event a owl:Class ;
    rdfs:subClassOf ifc:Process ;
    owl:disjointWith ifc:Procedure,
        ifc:Task .

ifc:EventTime a owl:Class ;
    rdfs:subClassOf ifc:SchedulingTime ;
    owl:disjointWith ifc:TaskTime .

ifc:Task a owl:Class ;
    rdfs:subClassOf ifc:Process ;
    owl:disjointWith ifc:Event,
        ifc:Procedure .

ifc:TaskTime a owl:Class ;
    rdfs:subClassOf ifc:SchedulingTime ;
    owl:disjointWith ifc:EventTime .

ifc:Process a owl:Class ;
    rdfs:subClassOf [ a owl:Class ;
            owl:unionOf ( ifc:Event ifc:Procedure ifc:Task ) ] .

